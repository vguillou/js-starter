{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@src": "./src",
          "@tests": "./src/__tests__"
        }
      }
    ]
  ]
}
